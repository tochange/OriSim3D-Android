<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android"
    package="rk.or.android"
    a:versionCode="5"
    a:versionName="OriSim3D-1.5" >

    <uses-sdk
        a:minSdkVersion="7"
        a:targetSdkVersion="22" />

    <supports-screens
        a:anyDensity="true"
        a:largeScreens="true"
        a:normalScreens="true"
        a:resizeable="true"
        a:smallScreens="true" />

    <uses-feature
        a:glEsVersion="0x00020000"
        a:required="true" />

    <application
        a:allowBackup="true"
        a:allowClearUserData="true"
        a:debuggable="true"
        a:icon="@drawable/ic_launcher_orisim"
        a:label="@string/orisim3d_app"
        a:process="rk.or.android.ModelSelection"
        a:testOnly="true" >
        <activity
            a:name="rk.or.android.ModelSelection"
            a:noHistory="false"
            a:stateNotNeeded="true" >
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />

                <category a:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            a:name="rk.or.android.ModelView"
            a:exported="false"
            a:noHistory="true"
            a:stateNotNeeded="true" >
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>